<?xml version="1.0" encoding="utf-8"?>
<layout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
                  android:layout_width="match_parent"
                  android:layout_height="match_parent"
                  android:divider="@drawable/linearlayout_divider_shape"
                  android:orientation="vertical"
                  android:showDividers="middle">

        <RelativeLayout
            android:id="@+id/setting_feedback_layout"
            style="@style/settings_item_layout_style">

            <TextView
                style="@style/settings_item_text_style"
                android:text="@string/setting_feedback"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/setting_open_source_layout"
            style="@style/settings_item_layout_style">

            <TextView
                style="@style/settings_item_text_style"
                android:text="@string/setting_open_source_license"/>

        </RelativeLayout>
    </LinearLayout>
</layout>